<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings file="">
    <clear />
    <!--  WebApiServerUrl：WEB服务器地址    -->
    <add key="WebApiServerUrl" value="http://bimapi.pinming.cn" />
    <add key="EnableWindowsFormsHighDpiAutoResizing" value="true" />
    <!--  CacheRootPath：缓存目录    -->
    <add key="CacheRootPath" value="Cache" />
    <!--  DownloadRootPath：下载目录    -->
    <add key="DownloadRootPath" value="我的下载" />
    <!--  DBType：本地数据库类型，合法的值有：Access,SQL2000,SQL2005,MySQL,SQLite-->
    <add key="DBType" value="SQLite" />
    <!--  上传设置 UploadTaskNum：同时上传任务数，取值范围[2,99]  UploadThreadNumPerTask：上传单任务线程数，如果为0，则表示智能，取值范围[0,10]-->
    <add key="UploadTaskNum" value="3" />
    <add key="UploadThreadNumPerTask" value="1" />
    <!--  下载设置 DownloadTaskNum：同时下载任务数，取值范围[2,99]  DownloadThreadNumPerTask：下载单任务线程数，如果为0，则表示智能，取值范围[0,10]-->
    <add key="DownloadTaskNum" value="3" />
    <add key="DownloadThreadNumPerTask" value="1" />
    <!--  SilentDownloadTaskNum：同时静默下载任务数，取值范围[1,5]-->
    <add key="SilentDownloadTaskNum" value="2" />
    <!--  ResultDownloadTaskNum：同时成果下载任务数，取值范围[2,99]或设置为0关闭-->
    <add key="ResultDownloadTaskNum" value="0" />
    <add key="DeleteTaskNum" value="2" />
    <!--SyncSubProjectTaskNum：同时同步工程任务线程数，取值范围[1,5]-->
    <add key="SyncSubProjectTaskNum" value="2" />
    <!--IsShowOtherList：是否显示其他任务列表，0：不显示 1：显示-->
    <add key="IsShowOtherList" value="0" />
    <!-- 使用多线程的最小文件大小，单位为M，最小为5-->
    <add key="MinFileSizeToUseMultiThreads" value="50"/>
  </appSettings>
  <runtime>
    <enforceFIPSPolicy enabled="false" />
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-6.0.0.0" newVersion="6.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <probing privatePath="CommonDLL" />
    </assemblyBinding>
  </runtime>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0" />
  </startup>
</configuration>